{
  "name": "@hijacker/plugin-frontend",
  "version": "1.0.0-beta.8",
  "type": "module",
  "main": "dist/index.js",
  "description": "Hijacker plugin that adds a web interface to add and modify rules.",
  "author": "Travis Weidenbenner <travis@weidenbenner.com>",
  "homepage": "http://hijacker.io",
  "license": "MIT",
  "repository": "https://github.com/travis-w/hijacker",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm build:back && pnpm build:front",
    "build:back": "tsc --project tsconfig.build.json",
    "build:front": "vite build",
    "dev": "vite",
    "dev:back": "tsc --project tsconfig.build.json --watch",
    "dev:front": "NODE_ENV=development vite build --watch --emptyOutDir",
    "lint": "eslint src/",
    "test": "vitest --coverage --config vitest.config.ts",
    "storybook": "storybook dev -p 6006 -c src/frontend/.storybook",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.48.1",
    "express": "^4.18.2",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@hijacker/core": "workspace:*",
    "@hijacker/eslint-config": "workspace:*",
    "@hijacker/tsconfig": "workspace:*",
    "@mantine/core": "^7.12.1",
    "@mantine/hooks": "^7.12.1",
    "@monaco-editor/react": "^4.6.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/preview-api": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-istanbul": "^0.34.6",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-storybook": "^0.8.0",
    "got": "^13.0.0",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.44.0",
    "nock": "^13.3.8",
    "postcss": "^8.4.41",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "socket.io-client": "^4.7.2",
    "storybook": "^8.2.9",
    "storybook-dark-mode": "^4.0.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "@hijacker/core": "workspace:*"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
